package Sort;



public class Mp_Sort {
	public static void main(String[] args) {
		int[] arr = {4,1,5,7,13,25,2,6};
		sort(arr);

}
	

	public static void sort(int[] arr) {
		for (int i = 0; i < arr.length-1; i++) {	
		    for (int j = 0; j < arr.length; j++) {
		    	if(arr[i]>arr[i+1]){
		    		int temp = arr[i];
		    		arr[i+1] = arr[i];
		    		arr[i] = temp;
		    	}
				
			}
			
		}
	   System.out.print(arr);
	}
}
